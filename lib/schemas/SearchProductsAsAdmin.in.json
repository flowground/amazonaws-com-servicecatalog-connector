{
    "type": "object",
    "properties": {
        "PageSize": {
            "required": false,
            "type": "string"
        },
        "PageToken": {
            "required": false,
            "type": "string"
        },
        "requestBody": {
            "properties": {
                "AcceptLanguage": {
                    "description": "<p>The language code.</p> <ul> <li> <p> <code>en</code> - English (default)</p> </li> <li> <p> <code>jp</code> - Japanese</p> </li> <li> <p> <code>zh</code> - Chinese</p> </li> </ul>",
                    "maxLength": 100,
                    "type": "string"
                },
                "Filters": {
                    "description": "The search filters. If no search filters are specified, the output includes all products to which the administrator has access.",
                    "items": {
                        "properties": {
                            "key": {
                                "enum": [
                                    "FullTextSearch",
                                    "Owner",
                                    "ProductType",
                                    "SourceProductId"
                                ],
                                "type": "string"
                            },
                            "value": {
                                "items": {
                                    "type": "string"
                                },
                                "type": "array"
                            }
                        },
                        "type": "object"
                    },
                    "type": "array"
                },
                "PageSize": {
                    "description": "The maximum number of items to return with this call.",
                    "maximum": 20,
                    "minimum": 0,
                    "type": "integer"
                },
                "PageToken": {
                    "description": "The page token for the next set of results. To retrieve the first set of results, use null.",
                    "maxLength": 2024,
                    "pattern": "[\\u0009\\u000a\\u000d\\u0020-\\uD7FF\\uE000-\\uFFFD]*",
                    "type": "string"
                },
                "PortfolioId": {
                    "description": "The portfolio identifier.",
                    "maxLength": 100,
                    "minLength": 1,
                    "pattern": "^[a-zA-Z0-9_\\-]*",
                    "type": "string"
                },
                "ProductSource": {
                    "description": "Access level of the source of the product.",
                    "enum": [
                        "ACCOUNT"
                    ],
                    "type": "string"
                },
                "SortBy": {
                    "description": "The sort field. If no value is specified, the results are not sorted.",
                    "enum": [
                        "Title",
                        "VersionCount",
                        "CreationDate"
                    ],
                    "type": "string"
                },
                "SortOrder": {
                    "description": "The sort order. If no value is specified, the results are not sorted.",
                    "enum": [
                        "ASCENDING",
                        "DESCENDING"
                    ],
                    "type": "string"
                }
            },
            "type": "object"
        },
        "X_Amz_Content_Sha256": {
            "required": false,
            "type": "string"
        },
        "X_Amz_Date": {
            "required": false,
            "type": "string"
        },
        "X_Amz_Algorithm": {
            "required": false,
            "type": "string"
        },
        "X_Amz_Credential": {
            "required": false,
            "type": "string"
        },
        "X_Amz_Security_Token": {
            "required": false,
            "type": "string"
        },
        "X_Amz_Signature": {
            "required": false,
            "type": "string"
        },
        "X_Amz_SignedHeaders": {
            "required": false,
            "type": "string"
        }
    }
}