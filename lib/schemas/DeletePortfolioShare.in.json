{
    "type": "object",
    "properties": {
        "Action": {
            "required": true,
            "type": "string"
        },
        "Version": {
            "required": true,
            "type": "string"
        },
        "requestBody": {
            "properties": {
                "AcceptLanguage": {
                    "description": "<p>The language code.</p> <ul> <li> <p> <code>en</code> - English (default)</p> </li> <li> <p> <code>jp</code> - Japanese</p> </li> <li> <p> <code>zh</code> - Chinese</p> </li> </ul>",
                    "maxLength": 100,
                    "type": "string"
                },
                "AccountId": {
                    "description": "The AWS account ID.",
                    "pattern": "^[0-9]{12}$",
                    "type": "string"
                },
                "OrganizationNode": {
                    "description": "The organization node to whom you are going to stop sharing.",
                    "properties": {
                        "Type": {
                            "description": "The organization node type.",
                            "enum": [
                                "ORGANIZATION",
                                "ORGANIZATIONAL_UNIT",
                                "ACCOUNT"
                            ],
                            "type": "string"
                        },
                        "Value": {
                            "description": "The identifier of the organization node.",
                            "pattern": "(^[0-9]{12}$)|(^arn:aws:organizations::\\d{12}:organization\\/o-[a-z0-9]{10,32})|(^o-[a-z0-9]{10,32}$)|(^arn:aws:organizations::\\d{12}:ou\\/o-[a-z0-9]{10,32}\\/ou-[0-9a-z]{4,32}-[0-9a-z]{8,32}$)|(^ou-[0-9a-z]{4,32}-[a-z0-9]{8,32}$)",
                            "type": "string"
                        }
                    },
                    "type": "object"
                },
                "PortfolioId": {
                    "description": "The portfolio identifier.",
                    "maxLength": 100,
                    "minLength": 1,
                    "pattern": "^[a-zA-Z0-9_\\-]*",
                    "type": "string"
                }
            },
            "required": [
                "PortfolioId"
            ],
            "type": "object"
        },
        "X_Amz_Content_Sha256": {
            "required": false,
            "type": "string"
        },
        "X_Amz_Date": {
            "required": false,
            "type": "string"
        },
        "X_Amz_Algorithm": {
            "required": false,
            "type": "string"
        },
        "X_Amz_Credential": {
            "required": false,
            "type": "string"
        },
        "X_Amz_Security_Token": {
            "required": false,
            "type": "string"
        },
        "X_Amz_Signature": {
            "required": false,
            "type": "string"
        },
        "X_Amz_SignedHeaders": {
            "required": false,
            "type": "string"
        }
    }
}